<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @Styles.Render("~/css/adminlte")

    @RenderSection("headerCss", false)

</head>
<body class="hold-transition sidebar-mini">

    @RenderBody()


    @Scripts.Render("~/bundles/jquery")

    <script>window.jQuery || document.write('<script src="@Url.Content("~/Content/plugins/jquery/jquery.min.js")">\x3C/script>')</script>

    @Scripts.Render("~/bundles/bootstrap4")

    @Scripts.Render("~/bundles/adminlte")

    @RenderSection("footerScript", false)
</body>
</html>
